{
  "name": "teckos-client",
  "version": "0.0.11",
  "description": "uWebsocket.js based client component supporting async callbacks",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "dev:web": "webpack serve --config webpack.config.example.web.js",
    "dev:node": "DEBUG=example* nodemon --watch './src/**/*.ts,./example/node/**/*.ts' --exec 'ts-node' --project tsconfig.example.node.json example/node/index.ts",
    "build:lib": "tsc --p tsconfig.json",
    "build:example:web": "webpack --config webpack.config.example.web.js",
    "build:example:node": "tsc --p tsconfig.example.node.json",
    "build:example": "npm run build:example:web && npm run build:example:node",
    "build": "npm run build:lib",
    "start:node": "npm run build:example:node && node ./dist/example/node/index.js",
    "lint": "npx eslint ./src --ext .js,.ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delude88/teckos-client.git"
  },
  "keywords": [
    "websocket",
    "uwebsocket",
    "realtime",
    "callbacks"
  ],
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delude88/teckos-client/issues"
  },
  "homepage": "https://github.com/delude88/teckos-client#readme",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "html-webpack-plugin": "^4.5.0",
    "nodemon": "^2.0.6",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "debug": "^4.2.0",
    "isomorphic-ws": "^4.0.1",
    "ws": "^7.4.0"
  }
}
