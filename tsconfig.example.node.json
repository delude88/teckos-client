{
  "compilerOptions": {
    "module": "node16",
    "moduleResolution": "node16",
    "target": "esnext",
    "sourceMap": true,
    "strict": true,
    "outDir": "example/dist/node",
    "esModuleInterop": true
  },
  "lib": [
    "es2015"
  ],
  "include": [
    "example/src/node/**/*.ts",
    "types/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
