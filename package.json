{
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "websocket",
    "uwebsocket",
    "realtime",
    "callbacks"
  ],
  "scripts": {
    "dev:web": "webpack serve --config webpack.config.example.web.js",
    "dev:node": "DEBUG=example*,teckos* nodemon --watch './src/**/*.ts,./example/node/**/*.ts' --exec 'ts-node' --project tsconfig.example.node.json example/node/index.ts",
    "build:example:web": "webpack --config webpack.config.example.web.js",
    "build:example:node": "tsc --p tsconfig.example.node.json",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "eslint src/**",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5"
  },
  "name": "teckos-client",
  "author": "Tobias Hegemann <tobias.hegemann@googlemail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delude88/teckos-client.git"
  },
  "homepage": "https://github.com/delude88/teckos-client#readme",
  "bugs": {
    "url": "https://github.com/delude88/teckos-client/issues"
  },
  "module": "dist/teckos-client.esm.js",
  "size-limit": [
    {
      "path": "dist/teckos-client.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/teckos-client.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.2",
    "@types/debug": "^4.1.7",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "size-limit": "^5.0.2",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "ws": "^8.0.0"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "isomorphic-ws": "^4.0.1"
  }
}
